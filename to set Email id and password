#include <stdio.h>
#include <string.h>

#define MAX_EMAIL_LENGTH 50
#define MAX_PASSWORD_LENGTH 20
void setEmail(char email[MAX_EMAIL_LENGTH]) {
    printf("Enter your email ID: ");
    scanf("%s", email);
}
void setPassword(char password[MAX_PASSWORD_LENGTH]) {
    printf("Enter your password: ");
    scanf("%s", password);
}
int main() {
    char email[MAX_EMAIL_LENGTH];
    char password[MAX_PASSWORD_LENGTH];

    setEmail(email);
    setPassword(password);

    printf("\nEmail ID set: %s\n", email);
    printf("Password set: %s\n", password);

    return 0;
}
