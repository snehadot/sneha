#include<stdio.h>

int main() {
    char name[] = "Sneha";
    char middleName[] = "Sanjay";
    char surname[] = "Nagthane";
    
    for (int i = 0; i < 6; i++) 
	{printf("%c", name[i]);
    }
    printf("\n");
    for (int i = 0; i < 9; i++) {
        printf("%c", middleName[i]);
    }
    printf("\n");
    for (int i = 0; i < 8; i++) 
	{printf("%c", surname[i]);
    }
    printf("\n");
return 0;
}
